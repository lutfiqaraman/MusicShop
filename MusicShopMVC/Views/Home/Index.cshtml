@model ListMusicViewModel

    <table>
        <thead>
            <tr>
                <th>Name of Music</th>
                <th>Artist</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var music in Model.Musics)
            {
                <tr>
                    <td class="text-center">@music.Name</td>
                    <td class="text-center">@music.Artist.Name</td>
                </tr>
            }
        </tbody>
    </table>